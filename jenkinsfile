pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('CheckOut'){
            steps{
                git url:"https://github.com/sh1vam31/final_DEVOPS.git",branch:'main'
            }
        }
        stage('build'){
            steps{
                bat 'mvn clean package'
            }
        }
        stage('test'){
            steps{
                bat 'mvn test'
            }
        }
    }
}